    "vscode": "^1.22.0"
  },
  "categories": ["Themes"],
  "contributes": {
    "themes": [
      {
        "label": "Nice Dark Theme",
        "uiTheme": "vs-dark",
        "path": "themes/nice-dark-color-theme-v1.1.json"

      },
      {
        "label": "Nice Dark Theme - purple syntax",
        "uiTheme": "vs-dark",
